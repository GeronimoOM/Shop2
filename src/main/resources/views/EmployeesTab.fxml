<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="-1.0" maxWidth="-1.0" minHeight="-1.0" minWidth="-1.0" prefHeight="-1.0" prefWidth="633.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="ukma.groupproject.shop.controller.EmployeesTabController">
  <bottom>
    <BorderPane prefHeight="66.0" prefWidth="1000.0">
      <center>
        <HBox alignment="CENTER_RIGHT" spacing="10.0">
          <children>
            <Button fx:id="createButton" mnemonicParsing="false" text="Create" />
            <Button fx:id="editButton" mnemonicParsing="false" text="Edit" />
            <Button id="deleteButton" fx:id="removeButton" mnemonicParsing="false" text="Remove" />
          </children>
        </HBox>
      </center>
      <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
    </BorderPane>
  </bottom>
  <center>
    <BorderPane prefHeight="200.0" prefWidth="200.0">
      <center>
        <TableView id="itemsTable" fx:id="employeesTable" prefWidth="1000.0">
          <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
          </columnResizePolicy>
          <columns>
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="383.0" resizable="true" text="Name" fx:id="nameColumn" />
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="232.0" resizable="true" text="Salary" fx:id="salaryColumn" />
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="194.0" resizable="true" text="Department" fx:id="departmentColumn" />
          </columns>
        </TableView>
      </center>
    </BorderPane>
  </center>
</BorderPane>
