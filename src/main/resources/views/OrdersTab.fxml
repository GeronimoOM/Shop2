<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="-1.0" maxWidth="-1.0" minHeight="-1.0" minWidth="-1.0" prefHeight="-1.0" prefWidth="804.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="ukma.groupproject.shop.controller.OrdersTabController">
  <bottom>
    <BorderPane prefHeight="74.0" prefWidth="1000.0">
      <center>
        <HBox alignment="CENTER_RIGHT" prefHeight="60.0" prefWidth="1000.0" spacing="10.0">
          <children>
            <Button fx:id="removeButton" mnemonicParsing="false" text="Remove" />
          </children>
          <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
          </padding>
        </HBox>
      </center>
    </BorderPane>
  </bottom>
  <center>
    <BorderPane prefHeight="200.0" prefWidth="200.0">
      <center>
        <TableView id="itemsTable" fx:id="ordersTable" prefWidth="1000.0">
          <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
          </columnResizePolicy>
          <columns>
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="242.0" resizable="false" text="Date" fx:id="dateColumn" />
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="151.0" resizable="false" text="Item" fx:id="itemColumn" />
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="151.0" resizable="false" text="Amount" fx:id="amountColumn" />
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="151.0" resizable="false" text="Supplier" fx:id="supplierColumn" />
          </columns>
        </TableView>
      </center>
    </BorderPane>
  </center>
</BorderPane>
