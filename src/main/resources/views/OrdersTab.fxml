<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="-1.0" maxWidth="-1.0" minHeight="-1.0" minWidth="-1.0" prefHeight="-1.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ukma.groupproject.shop.controller.OrdersTabController">
  <bottom>
    <BorderPane>
      <center>
        <HBox alignment="CENTER_RIGHT" spacing="10.0">
          <children>
            <Button fx:id="removeButton" mnemonicParsing="false" text="Remove" />
          </children>
          <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
          </padding>
        </HBox>
      </center>
         <padding>
            <Insets top="10.0" />
         </padding>
    </BorderPane>
  </bottom>
  <center>
    <BorderPane>
      <center>
        <TableView id="itemsTable" fx:id="ordersTable">
          <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
          </columnResizePolicy>
          <columns>
            <TableColumn fx:id="dateColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="242.0" resizable="false" text="Date" />
            <TableColumn fx:id="itemColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="151.0" resizable="false" text="Item" />
            <TableColumn fx:id="amountColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="151.0" resizable="false" text="Amount" />
            <TableColumn fx:id="supplierColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="151.0" resizable="false" text="Supplier" />
          </columns>
        </TableView>
      </center>
    </BorderPane>
  </center>
</BorderPane>
