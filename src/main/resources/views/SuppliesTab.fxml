<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="-1.0" maxWidth="-1.0" minHeight="-1.0" minWidth="-1.0" prefHeight="-1.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ukma.groupproject.shop.controller.SuppliesTabController">
  <bottom>
    <HBox alignment="CENTER_RIGHT" prefHeight="47.0" prefWidth="600.0" spacing="10.0">
      <children>
        <Button fx:id="createButton" mnemonicParsing="false" text="Create" />
        <Button fx:id="editButton" mnemonicParsing="false" text="Edit" />
        <Button fx:id="removeButton" mnemonicParsing="false" text="Remove" />
      </children>
      <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
    </HBox>
  </bottom>
  <center>
    <BorderPane>
      <center>
        <TableView fx:id="suppliesTable" prefWidth="1000.0">
               <columns>
                  <TableColumn fx:id="dateColumn" text="Date" />
                  <TableColumn fx:id="supplierColumn" prefWidth="75.0" text="Supplier" />
                  <TableColumn fx:id="itemsColumn" prefWidth="75.0" text="Items" />
               </columns></TableView>
      </center>
    </BorderPane>
  </center>
</BorderPane>
